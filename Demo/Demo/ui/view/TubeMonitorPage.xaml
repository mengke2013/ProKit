                            <Border Margin="0,0,0,0" Width="80" Height="40" HorizontalAlignment="Center" VerticalAlignment="Top" BorderBrush="#FF301313" Background="#FFD3C7C7" BorderThickness="2">
                                <TextBlock Margin="0,0,0,0" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="14" FontWeight="Bold" Foreground="#FF646366">Condenser</TextBlock>
                            </Border>
                        </StackPanel>
                    </Grid>
                    <Grid Grid.Row="2" Grid.Column="4" Grid.ColumnSpan="3" Grid.RowSpan="3">
                        <Rectangle Margin="0,0,0,0" Width="2" Height="35" VerticalAlignment="Top" Stroke="#FF301313" Fill="#FFD3C7C7" StrokeThickness="2"></Rectangle>
                        <StackPanel>
                            <Rectangle Margin="0,0,0,5" Width="10" VerticalAlignment="Top" Height="30" Stroke="#FF301313" Fill="#FFD3C7C7" StrokeThickness="2"></Rectangle>
                            <TextBlock Margin="0,0,0,5" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="14" FontWeight="Bold" Foreground="#FF646366">Tube Presure</TextBlock>
                            <controls:ClickSelectTextBox Margin="0,0,0,5" Style="{Binding Path=TubePageStyle.TextBoxStyle}" Text="{Binding  Path=Ana1CurMeas}"></controls:ClickSelectTextBox>
                            <controls:ClickSelectTextBox Style="{Binding Path=TubePageStyle.TextBoxStyle}" Text="{Binding  Path=Ana1Sp}" Visibility="{Binding Path=EditVisible}"></controls:ClickSelectTextBox>
                        </StackPanel>
                    </Grid>
                    <Grid Grid.Row="2" Grid.Column="6" Grid.ColumnSpan="3" Grid.RowSpan="3">
                        <StackPanel>
                            <Border Margin="0,-10,0,0" Width="80" Height="40"  VerticalAlignment="Top" BorderBrush="#FF301313" Background="#FF00ff00" BorderThickness="2">
                                <WrapPanel Margin="0,5" HorizontalAlignment="Center" VerticalAlignment="Center">
                                    <local:DioSwitcher Margin="-15,0,0,0" Width="15" Height="15" HorizontalAlignment="Left"  Value="{Binding Path=Do6}" Visibility="{Binding Path=EditVisible}"/>
                                    <Rectangle Margin="2,0,5,0"  Width="5" Height="5"  Stroke="#FF301313" Fill="{Binding Path=Do6Color}" StrokeThickness="1"></Rectangle>
                                    <TextBlock Margin="0,0,0,0" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="14" FontWeight="Bold" Foreground="#FF646366">Gate</TextBlock>
                                </WrapPanel>
                            </Border>
                            <controls:ClickSelectTextBox Margin="0,5,0,0" Style="{Binding Path=TubePageStyle.TextBoxStyle}" Text="{Binding  Path=Ana4CurMeas}"></controls:ClickSelectTextBox>
                            <Button Margin="0,5,0,0" Width="70" Height="30"  Content="PID" FontSize="14" FontWeight="Bold" Foreground="#FF646366"></Button>
                        </StackPanel>
                    </Grid>
                    <Grid Grid.Row="2" Grid.Column="8" Grid.ColumnSpan="3" Grid.RowSpan="3">
                        <Rectangle Margin="0,0,0,0" Width="2" Height="35" VerticalAlignment="Top"  Stroke="#FF301313" Fill="#FFD3C7C7" StrokeThickness="2"></Rectangle>
                        <StackPanel>
                            <Rectangle Margin="0,0,0,5" Width="10" VerticalAlignment="Top" Height="30" Stroke="#FF301313" Fill="#FFD3C7C7" StrokeThickness="2"></Rectangle>
                            <TextBlock Margin="0,0,0,5" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="14" FontWeight="Bold" Foreground="#FF646366">Ballast Presure</TextBlock>
                            <controls:ClickSelectTextBox Style="{Binding Path=TubePageStyle.TextBoxStyle}" Text="{Binding  Path=Ana3CurMeas}"></controls:ClickSelectTextBox>
                        </StackPanel>
                    </Grid>
                    <Grid Grid.Row="2" Grid.Column="11" Grid.ColumnSpan="1" Grid.RowSpan="3">
                        <StackPanel>
                            <Rectangle Margin="0,0,0,0" Width="60" Height="20" HorizontalAlignment="Center"  VerticalAlignment="Top" Stroke="#FF301313" Fill="#FFD3C7C7" StrokeThickness="2"></Rectangle>
                            <Border Margin="0,0,0,0" Width="80" Height="40" HorizontalAlignment="Center" VerticalAlignment="Top" BorderBrush="#FF301313" Background="#FFD3C7C7" BorderThickness="2">
                                <WrapPanel Margin="10,0,0,0"  HorizontalAlignment="Center" VerticalAlignment="Center">
                                    <local:DioSwitcher Margin="-15,0,0,0" Width="15" Height="15" HorizontalAlignment="Left"  Value="{Binding Path=Do7}" Visibility="{Binding Path=EditVisible}"/>
                                    <Rectangle Margin="2,0,5,0"  Width="5" Height="5"  Stroke="#FF301313" Fill="{Binding Path=Do7Color}" StrokeThickness="2"></Rectangle>
                                    <TextBlock Margin="0,0,0,0" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="14" FontWeight="Bold" Foreground="#FF646366">Pump</TextBlock>
                                </WrapPanel>
                            </Border>
                        </StackPanel>
                    </Grid>
                    <Rectangle Margin="0,6,0,0" Grid.Row="2" Grid.Column="12" Width="10" Height="10" HorizontalAlignment="Right" VerticalAlignment="Top" Stroke="#FF301313" Fill="#FFD3C7C7" StrokeThickness="2"></Rectangle>
                </Grid>
            </Grid>
        </Grid>
    </DockPanel>
</UserControl>
