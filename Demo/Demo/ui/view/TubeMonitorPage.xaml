<UserControl x:Class="Demo.ui.TubeMonitorPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Demo.ui.view"
             xmlns:controls="clr-namespace:Demo.ui.controls"
             mc:Ignorable="d" 
             d:DesignHeight="600" d:DesignWidth="950">
    <DockPanel>
        <Grid DockPanel.Dock="Top">
            <DockPanel >
                <Border DockPanel.Dock="Left" Height="30" Width="25">
                    <!--<TextBlock Margin="30,0,15,0" HorizontalAlignment="Left" VerticalAlignment="Bottom" FontSize="16" Foreground="#FF236EC9" FontWeight="Bold">Recipe Demo</TextBlock>-->
                </Border>
                <Button DockPanel.Dock="Right" Margin="5" VerticalAlignment="Top" Click="CloseButton_Click" >
                    <Image Source="/Demo;component/images/close.PNG"  Width="20" Height="20"></Image>
                </Button>
                <Grid >
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="210"/>
                        <ColumnDefinition />
                        <ColumnDefinition />
                        <ColumnDefinition Width="140"/>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition />
                    </Grid.RowDefinitions>
                    <WrapPanel Margin="5,8,5,2" VerticalAlignment="Bottom" Grid.Column="0" Grid.ColumnSpan="1">
                        <Button Name="btnStart" Click="StartButton_Click" Width="40" Height="40">
                            <Image Source="/Demo;component/images/play.png" ></Image>
                        </Button>
                        <Button Name="btnHold" Click="HoldButton_Click" Width="40" Height="40">
                            <Image Source="/Demo;component/images/pause.png" ></Image>
                        </Button>
                        <Button Name="btnNext" Click="NextButton_Click" Width="40" Height="40">
                            <Image Source="/Demo;component/images/next.png" ></Image>
                        </Button>
                        <Button Name="btnIdle" Click="IdleButton_Click" Width="40" Height="40">
                            <Image Source="/Demo;component/images/abort.png" ></Image>
                        </Button>
                        <Button Name="btnAbort" Click="AbortButton_Click" Width="40" Height="40">
                            <Image Source="/Demo;component/images/idle.png" ></Image>
                        </Button>
                    </WrapPanel>
                    <ProgressBar Name="pgbProcess" Margin="5,0,5,2" Grid.Column="1" Grid.ColumnSpan="2" VerticalAlignment="Bottom" Height="8" Maximum="{Binding Path=MaxValue}" Value="{Binding Path=Progress}"></ProgressBar>
                    <WrapPanel Margin="10,8,10,2" Grid.Column="1" Grid.ColumnSpan="2" HorizontalAlignment="Left" VerticalAlignment="Center">
                        <TextBlock   Text="{Binding Path=ProcessName}" FontSize="16" Foreground="#FF236EC9" FontWeight="Bold"></TextBlock>
                        <TextBlock Margin="10,0,10,0" FontSize="16" Foreground="#FF236EC9" FontWeight="Bold" Text="/"></TextBlock>
                        <TextBlock Margin="0,0,0,0" Text="{Binding Path=StepName}" FontSize="16" Foreground="#FF236EC9" FontWeight="Bold"></TextBlock>
                        <TextBlock Margin="5,0,0,0" Text="{Binding Path=StepTimeS}" VerticalAlignment="Bottom" FontSize="12" Foreground="#FF236EC9" FontWeight="Bold"></TextBlock>
                       
                    </WrapPanel>
                    <WrapPanel Margin="10,8,10,2" Grid.Column="1" Grid.ColumnSpan="2" HorizontalAlignment="Right" VerticalAlignment="Center">
                        <TextBlock Margin="10,0,0,0" Text="{Binding Path=ProcessStatus}" FontSize="16" Foreground="#FF236EC9" FontWeight="Bold"></TextBlock>
                        <TextBlock Margin="10,0,0,0" Text="{Binding Path=ProcessRemainingTimeS}" VerticalAlignment="Bottom" FontSize="12" Foreground="#FF236EC9" FontWeight="Bold"></TextBlock>
                    </WrapPanel>
                    <WrapPanel Margin="5,8,5,2" Grid.Column="3" HorizontalAlignment="Right" VerticalAlignment="Bottom">
                        <Button  Margin="0,0,5,0" Click="SettingsButton_Click" Width="40" Height="40">
                            <Image Source="/Demo;component/images/configuration.png"></Image>
                        </Button>
                        <Button  Margin="0,0,5,0" Width="40" Height="40">
                            <Image Source="/Demo;component/images/maintenance.png"></Image>
                        </Button>
                        <Button Margin="0,0,0,0" Width="40" Height="40">
                            <Image Source="/Demo;component/images/alarm.png"></Image>
                        </Button>

                    </WrapPanel>
                </Grid>
            </DockPanel>
        </Grid>
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition MaxHeight="5" />
                <RowDefinition />
                <RowDefinition MaxHeight="10" />
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition MaxWidth="10"/>
                <ColumnDefinition/>
                <ColumnDefinition MaxWidth="10"/>
            </Grid.ColumnDefinitions>
            <Rectangle Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="3" Margin="30,0,30,2" Height="1" Fill="#FFAAA5A5" />
            <Grid Margin="10" Grid.Row="1" Grid.ColumnSpan="2" Grid.RowSpan="2">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition />
                    <ColumnDefinition />
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                </Grid.RowDefinitions>
                <Grid Grid.Row="0" Grid.Column="0" Grid.RowSpan="5" Grid.ColumnSpan="1" Margin="0,0,0,0">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="0.5*"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="0.5*"/>
                        <ColumnDefinition Width="0.3*"/>
                        <ColumnDefinition Width="0.3*"/>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="0.2*"/>
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition Height="1.5*"/>
                        <RowDefinition Height="1.5*"/>
                        <RowDefinition Height="1*"/>
                        <RowDefinition />
                        <RowDefinition Height="1.5*"/>
                    </Grid.RowDefinitions>

                    <Grid Grid.Row="1" Grid.Column="1" Grid.ColumnSpan="9" Grid.RowSpan="7">
                        <Rectangle Margin="-15,0,0,0" Height="10" Width="30" HorizontalAlignment="Left" VerticalAlignment="Top" Stroke="#FF301313" Fill="{Binding Path=Pipe14Color}" StrokeThickness="2"></Rectangle>
                        <Border Margin="-5,8,0,12"  Width="10" HorizontalAlignment="Left" BorderBrush="#FF301313" Background="{Binding Path=Pipe14Color}" BorderThickness="2,0,2,2"></Border>
                        <Border Margin="3,0,0,12"  Height="10"   VerticalAlignment="Bottom" BorderBrush="#FF301313" Background="{Binding Path=Pipe14Color}" BorderThickness="0,2,2,2"></Border>
                    </Grid>
                    <StackPanel Grid.Row="6" Grid.Column="1" Grid.RowSpan="2" Grid.ColumnSpan="1">
                        <WrapPanel>
                            <Rectangle Margin="-10,-15,0,0"  Width="20" Height="30" HorizontalAlignment="Left" VerticalAlignment="Top" Stroke="#FF301313" Fill="{Binding Path=Ev14Color}" StrokeThickness="2"></Rectangle>
                            <local:DioSwitcher Margin="5,-10,0,0" Width="20" Height="20" HorizontalAlignment="Left" VerticalAlignment="Top" Value="{Binding Path=Ev14}" Visibility="{Binding Path=EditVisible}"/>
                        </WrapPanel>
                        <Label Margin="-30,5,0,0" Width="60" HorizontalAlignment="Left" HorizontalContentAlignment="Center" Background="White" BorderBrush="Black" BorderThickness="1">N2 High</Label>

                    </StackPanel>
                    <Rectangle Margin="0,0,-10,17" Grid.Row="1" Grid.Column="9" Grid.RowSpan="7" Grid.ColumnSpan="2"  Height="2" VerticalAlignment="Bottom"  Stroke="#FF301313" Fill="#FFD3C7C7" StrokeThickness="2"></Rectangle>

                    <Grid Grid.Row="1" Grid.Column="2" Grid.ColumnSpan="8" Grid.RowSpan="7">
                        <Rectangle Margin="-15,0,0,0" Height="10" Width="30" HorizontalAlignment="Left" VerticalAlignment="Top" Stroke="#FF301313" Fill="{Binding Path=Pipe2Color}" StrokeThickness="2"></Rectangle>
                        <Border Margin="-5,8,0,24"  Width="10" HorizontalAlignment="Left"  BorderBrush="#FF301313" Background="{Binding Path=Pipe2Color}" BorderThickness="2,0,2,2"></Border>
                        <Border Margin="3,0,0,24"  Height="10"   VerticalAlignment="Bottom" BorderBrush="#FF301313" Background="{Binding Path=Pipe2Color}" BorderThickness="0,2,2,2"></Border>
                    </Grid>
                    <StackPanel  Grid.Row="6" Grid.Column="2" Grid.RowSpan="2" Grid.ColumnSpan="1">
                        <!--<Image Margin="-10,-15,35,0"  Width="30" Height="30" HorizontalAlignment="Right" VerticalAlignment="Top" Source="/Demo;component/images/gasValve1.bmp"></Image>-->
                        <WrapPanel>
                            <Rectangle Margin="-10,-15,0,0"  Width="20" Height="30" HorizontalAlignment="Left" VerticalAlignment="Top" Stroke="#FF301313" Fill="{Binding Path=Ev2Color}" StrokeThickness="2"></Rectangle>
                            <local:DioSwitcher Margin="5,-10,0,0" Width="20" Height="20" HorizontalAlignment="Left" VerticalAlignment="Top" Value="{Binding Path=Ev2}" Visibility="{Binding Path=EditVisible}"/>
                        </WrapPanel>
                        <Label Margin="-20,5,0,0" Width="40" HorizontalAlignment="Left" HorizontalContentAlignment="Center" Background="White" BorderBrush="Black" BorderThickness="1">H2</Label>
                    </StackPanel>

                    <StackPanel Grid.Row="3" Grid.Column="1" Grid.RowSpan="2" Grid.ColumnSpan="2" VerticalAlignment="Center">
                        <Border Margin="0,0,0,0"  Width="40" Height="30"  BorderBrush="#FF301313" Background="#FFD3C7C7" BorderThickness="2">
                            <TextBlock Margin="0,0,0,0" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="14" FontWeight="Bold" Foreground="#FF646366">H2</TextBlock>
                        </Border>
                        <controls:ClickSelectTextBox Style="{Binding Path=TubePageStyle.TextBoxStyle}" Margin="0,5,0,0" Text="{Binding  Path=Gas2Sp}" Visibility="{Binding Path=EditVisible}"></controls:ClickSelectTextBox>
                        <TextBox Style="{Binding Path=TubePageStyle.TextBoxStyle}" Margin="0,5,0,0" Text="{Binding  Path=Gas2CurMeas}"></TextBox>
                    </StackPanel>

                    <Rectangle Margin="0,0,0, 29" Grid.Row="1" Grid.Column="9" Grid.RowSpan="7" Grid.ColumnSpan="2"  Height="2" VerticalAlignment="Bottom"  Stroke="#FF301313" Fill="#FFD3C7C7" StrokeThickness="2"></Rectangle>

                    <Grid Grid.Row="1" Grid.Column="3" Grid.ColumnSpan="7" Grid.RowSpan="7">
                        <Rectangle Margin="-15,0,0,0" Height="10" Width="30" HorizontalAlignment="Left" VerticalAlignment="Top" Stroke="#FF301313" Fill="{Binding Path=Pipe1Color}" StrokeThickness="2"></Rectangle>
                        <Border Margin="-5,8,0,36"  Width="10" HorizontalAlignment="Left"  BorderBrush="#FF301313" Background="{Binding Path=Pipe1Color}" BorderThickness="2,0,2,2"></Border>
                        <Border Margin="3,0,0,36"  Height="10"   VerticalAlignment="Bottom" BorderBrush="#FF301313" Background="{Binding Path=Pipe1Color}" BorderThickness="0,2,2,2"></Border>
                    </Grid>
                    <StackPanel Grid.Row="6" Grid.Column="3" Grid.RowSpan="2" Grid.ColumnSpan="1">
